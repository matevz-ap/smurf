{
    "manifest_version": 3,
    "name": "Smurf",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["https://*/"],
    "permissions": [
        "webRequest",
        "*://*.google.com/",
        "declarativeNetRequest"
      ]
}